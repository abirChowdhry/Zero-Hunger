
@{
    ViewBag.Title = "Employees";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Employees</h2>

<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Phone</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Phone</td>

            <td>
                <a class="btn btn-success" href="MakeActive/@item.Id">Make Active</a>
            </td>
        </tr>
    }
</table>

